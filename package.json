{
  "name": "script",
  "license": "MIT",
  "scripts": {
    "dev": "ts-node --trace-warnings --loader ts-node/esm src/ts/apiRequest.ts",
    "start": "concurrently \"npm run watch\" \"npm run serve\"",
    "start:dev": "nodemon",
    "serve": "nodemon -w build/index.js",
    "watch": "npm run clean && tsc -w",
    "clean": "rimraf ./build",
    "tsc": "tsc --traceResolution -p tsconfig.json"
  },
  "engines": {
    "node": ">=16.0.0"
  },
  "type": "module",
  "devDependencies": {
    "@types/node": "^17.0.23",
    "nodemon": "^2.0.15",
    "prisma": "3.10.0",
    "ts-node": "^10.7.0",
    "tslint": "^6.1.3",
    "typescript": "^4.6.3"
  },
  "dependencies": {
    "@prisma/client": "3.10.0",
    "log4js": "^6.4.4",
    "node-fetch": "^3.2.3",
    "proxy-agent": "^5.0.0"
  }
}
